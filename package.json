  "scripts": {
    "build": "gatsby build",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "gatsby develop -H 0.0.0.0",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "storybook": "start-storybook -p 6006 -s ./static",
    "build-storybook": "build-storybook -c .storybook -o public/storybook",
    "test": "jest --watch --detectOpenHandles",
    "type-check": "tsc --noEmit",
    "types-generate": "graphql-codegen --config codegen.yml",
    "types-fetch": "node ./gatsby-fetch-apollo-cache-types.js",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --cache --fix",
      "jest --bail --findRelatedTests"
    ],
    "*.{js,jsx,ts,tsx,json,md}": "prettier --write"
  },
  "#husky": {
    "hooks": {
      "pre-commit": ""
    }
